<form id="calendar-form" class="view form">
  <input name="calendar_link[id]" type="hidden" value="{{id}}">
  <label for="name">Name</label>
  <input name="calendar_link[name]" value="{{name}}" type="text" class="form-control">
  <label for="url">URL</label>
  <input name="calendar_link[url]" value="{{url}}" type="text" class="form-control">
  <input type="submit" value="Update" class="btn btn-primary btn-large button">
</form>
